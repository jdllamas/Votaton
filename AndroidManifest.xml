<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vcc.votaton"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.vcc.votaton.MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vcc.votaton.EstadisticasActivity"
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name="com.vcc.votaton.EstadisticasDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName="com.vcc.votaton.EstadisticasActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vcc.votaton.ItemListActivity" />
        </activity>
        <activity
            android:name="com.vcc.votaton.EstadisticasListActivity"
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name="com.vcc.votaton.VotosActivity"
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name="com.vcc.votaton.VotosDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName="com.vcc.votaton.VotosActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vcc.votaton.ItemListActivity" />
        </activity>
        <activity
            android:name="com.vcc.votaton.VotosListActivity"            
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name="com.vcc.votaton.VotoListActivity"
            android:label="@string/title_voto_list" >
        </activity>
        <activity
            android:name="com.vcc.votaton.VotoDetailActivity"
            android:label="@string/title_voto_detail"
            android:parentActivityName="com.vcc.votaton.VotoListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vcc.votaton.VotoListActivity" />
        </activity>
        <activity
            android:name="com.vcc.votaton.PartidosActivity"
            android:label="@string/title_activity_partidos"
            android:configChanges="orientation"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.vcc.votaton.CandidatoActivity"
            android:label="@string/title_activity_candidato"
            android:configChanges="orientation"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>