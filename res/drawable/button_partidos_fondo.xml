<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape                		
    		android:shape="rectangle"    		
    	>    	    
			<corners
			    android:bottomRightRadius="5dp"
            	android:bottomLeftRadius="5dp"
            	android:topLeftRadius="5dp"
            	android:topRightRadius="5dp"				
			/>
			<padding
				android:left="0dp"
				android:top="0dp"
				android:right="0dp"
				android:bottom="0dp"
			/>
			<size
				android:width="150dp"
				android:height="150dp"
			/>
			<stroke
				android:width="7dp"
				android:color="#878787"
			/>
		</shape>	
	</item>
	<item>        
        <bitmap android:src="@drawable/tarj"            
            />
        <padding
            android:left="20dp"
			android:top="20dp"
			android:right="20dp"
			android:bottom="20dp"
            />
    </item>
</layer-list>	