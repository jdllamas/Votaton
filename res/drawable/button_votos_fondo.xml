<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape    		
    		android:shape="rectangle"     
    	>    
			<corners
				android:radius="30dp"
			/>
			<padding
				android:left="20dp"
				android:top="20dp"
				android:right="20dp"
				android:bottom="20dp"
			/>
			<size
				android:width="150dp"
				android:height="150dp"
			/>
			<stroke
				android:width="3dp"
				android:color="#878787"
			/>
		</shape>	
	</item>
    <item>        
        <bitmap android:src="@drawable/vote" />
    </item>
</layer-list>	